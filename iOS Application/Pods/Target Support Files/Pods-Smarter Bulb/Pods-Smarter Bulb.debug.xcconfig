ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/CULColorPicker" "${PODS_CONFIGURATION_BUILD_DIR}/HapticButton" "${PODS_CONFIGURATION_BUILD_DIR}/NotchyAlert" "${PODS_CONFIGURATION_BUILD_DIR}/Slope"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/CULColorPicker/CULColorPicker.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/HapticButton/HapticButton.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/NotchyAlert/NotchyAlert.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/Slope/Slope.framework/Headers"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_LDFLAGS = $(inherited) -framework "CULColorPicker" -framework "HapticButton" -framework "NotchyAlert" -framework "Slope" -framework "UIKit"
OTHER_SWIFT_FLAGS = $(inherited) -D COCOAPODS
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
